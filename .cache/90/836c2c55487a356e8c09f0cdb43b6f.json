t litMatch from '../assets/matches/lit-match.png';\n\nconst Match = ({\n    wallet,\n    currentPlayer,\n    row,\n    index,\n    gameState, \n    onSelect, \n    selectedRow, \n    selectedMatchIndex, \n    setSelectedMatchIndex \n}) => {\n    const [curImage, setCurImage] = useState(litMatch);\n\n    useEffect(() => {\n        if (gameState[row] != undefined && (selectedRow != row || index != selectedMatchIndex)) {\n            setCurImage(gameState[row] >= index ? litMatch : burntMatch)\n        }\n    }, [gameState, selectedRow]);\n\n    const imageClick = () => {\n        if(!wallet.getAccountId()) {\n            alert(\"Must Sign In To Play...\")\n            return;\n        }\n        if(wallet.getAccountId() != currentPlayer) {\n            alert(\"Only the active player can make a move...\")\n            return;\n        }\n        \n        setCurImage(burntMatch)\n        onSelect(row)\n        setSelectedMatchIndex(index)\n    }\n\n    return (\n        curImage == litMatch ?\n            <img\n                src={litMatch}\n                onClick={() => imageClick()}\n                style={{ 'cursor': 'pointer' }}\n                width={25}\n                height={125}\n            />\n            :\n            <img\n                src={burntMatch}\n                width={25}\n                height={127}\n            />\n    );\n};\n\nexport default Match;"},"lineCount":null}},"error":null,"hash":"9fee761655c36460924e7e4625c6a0ab","cacheData":{"env":{}}}